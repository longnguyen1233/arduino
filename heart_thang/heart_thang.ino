int dl = 5;
int xung =6;
int chot =7;

void doc_bit(int k)
{
  int i,j;
  byte b;
  digitalWrite(xung,0);
  digitalWrite(chot,0);
  for(i=3;i>=0; i--)
    for(j=0; j<=7; j++)
    {
      b = bitRead(data[k][i],j);
      digitalWrite(dl,b);
      digitalWrite(xung,1);
      delay(1);
      digitalWrite(xung,0);
    }
    digitalWrite(chot,1);
    delay(1);
digitalWrite(chot,0);
}
void setup(){
  pinMode(dl, OUTPUT);
  pinMode(xung, OUTPUT);
  pinMode(chot, OUTPUT);
}

void loop(){
  int k;
  for (k=0; k<=262; k++) {doc_bit(k); delay(100);}
}